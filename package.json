{
  "name": "electron-application-template",
  "version": "0.1.0",
  "private": true,
  "main": "build/electron/main.js",
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@apollo/react-hoc": "^4.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/recompose": "^0.30.7",
    "antd": "^4.10.0",
    "data-store": "^4.0.3",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.11.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "recompose": "^0.30.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start:electron": "NODE_ENV='development' electron .",
    "start:react": "BROWSER=none react-scripts start",
    "start": "concurrently 'yarn start:react' 'yarn start:electron'",
    "build:react": "PUBLIC_URL='.' react-scripts build",
    "build:electron": "tsc --project electron/tsconfig.json",
    "build": "npm run build:react && npm run build:electron",
    "pack": "electron-builder",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron": "^11.1.1",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.6"
  },
  "build": {
    "extends": null,
    "appId": "example.application",
    "mac": {
      "category": "example.app.category.type"
    },
    "files": [
      "build/**/*",
      "package.json"
    ]
  }
}
